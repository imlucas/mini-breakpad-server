{
  "name": "mini-breakpad-server",
  "description": "Minimum breakpad crash reports collecting server",
  "version": "0.2.0",
  "bin": {
    "mini-breakpad-server": "bin/mini-breakpad-server"
  },
  "scripts": {
    "prepublish": "grunt coffee",
    "dist": "lone"
  },
  "lone": {
    "ignore": [
      "src/*.coffee",
      "node_modules/coffee-script/**",
      "node_modules/grunt/**",
      "node_modules/grunt-cli/**",
      "node_modules/grunt-coffeelint/**",
      "node_modules/grunt-contrib-coffee/**",
      "node_modules/grunt-shell/**",
      "node_modules/lone/**"
    ]
  },
  "dependencies": {
    "decompress-zip": "0.0.4",
    "dirty": "0.9.7",
    "express": "3.x",
    "formidable": "~1.0.14",
    "fs-plus": "0.10.0",
    "github-releases": "0.1.x",
    "glob": "3.x",
    "jade": "~0.35.0",
    "minidump": "0.3.0",
    "mkdirp": "~0.3.5",
    "mongodb": "^1.4.7",
    "node-uuid": "~1.4.1",
    "temp": "0.7.0",
    "wrench": "1.5.x"
  },
  "devDependencies": {
    "coffee-script": "~1.6.2",
    "grunt": "~0.4.1",
    "grunt-cli": "~0.1.7",
    "grunt-coffeelint": "~0.0.6",
    "grunt-contrib-coffee": "~0.6.6",
    "grunt-shell": "~0.2.2",
    "lone": "^0.3.1",
    "rimraf": "~2.1.4"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/atom/mini-breakpad-server/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/atom/mini-breakpad-server.git"
  },
  "bugs": {
    "url": "https://github.com/atom/mini-breakpad-server/issues"
  }
}
